{
  "name": "@grpc-bridge/shared",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/shared/src",
  "projectType": "library",
  "tags": ["type:lib", "scope:shared"],
  "targets": {
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "eslint libs/shared/src",
        "cwd": "{workspaceRoot}"
      }
    },
    "type-check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit -p libs/shared/tsconfig.json",
        "cwd": "{workspaceRoot}"
      }
    }
  }
}
